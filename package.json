    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^10.0.0",
    "eslint-config-next": "15.2.6",
    "netlify-cli": "^23.15.1",
    "postcss": "^8.4.47",
