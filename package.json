    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.90.21",
    "@thirdweb-dev/storage": "^2.0.15",
    "@twa-dev/sdk": "^8.0.2",
    "clanker-sdk": "^4.2.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.2",
    "lucide-react": "^0.574.0",
